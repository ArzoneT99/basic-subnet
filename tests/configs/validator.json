{
    "neuron": {
      "name": "validator_neuron",
      "path": "neurons.validator.Validator"
    },
    "axon": {
      "port": 8091,
      "ip": "127.0.0.1",
      "max_workers": 10
    },
    "dendrite": {
      "timeout": 10.0,
      "max_active_synapses": 100,
      "max_active_receptors": 10
    },
    "metagraph": {
      "chain_endpoint": "mock_endpoint",
      "polls_every_sec": 30
    },
    "subtensor": {
      "network": "mock",
      "chain_endpoint": "mock_endpoint"
    },
    "wallet": {
      "name": "validator_wallet",
      "hotkey": "mock_hotkey",
      "path": null
    },
    "logging": {
      "console": {
        "level": "INFO"
      },
      "file": {
        "level": "DEBUG",
        "path": "validator.log",
        "max_size": 10485760,
        "backup_count": 10
      }
    }
  }
  